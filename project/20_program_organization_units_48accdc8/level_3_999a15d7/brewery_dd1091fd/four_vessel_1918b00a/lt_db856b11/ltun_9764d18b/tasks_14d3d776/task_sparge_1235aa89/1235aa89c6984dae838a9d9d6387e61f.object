.Transfer_Sparge_Validate:\n\t\tSparge.Target_Rate:=Targets.Sparge.Rate;\n\t\tSparge.Target_Temp:=Targets.Sparge.Temperature;\n\t\tSparge.Target_Volume:=Targets.Sparge.Volume;\n\t\tRake.PLC_CMD:=Targets.Sparge.Agitator_Speed;\n\t\tConfig.LTun.Task_Eta:=0;\n\t\tNext_Status:=eLT_Status.Transfer_Sparge_Start;\n\t\tTask_Ton.IN:=FALSE;\n\t\ttVolume:=Discharge_Flow.PDO.Totaliser_1.EU;\n\t\t\n\teLT_Status.Transfer_Sparge_Start:\n\t\tIF Sparge.Active AND Discharge_Flow.PDO.VolumeFlow.EU>Targets.Min_FloW THEN\n\t\t\tNext_Status:=eLT_Status.Transfer_Sparge_Active;\n\t\tEND_IF;\n\t//Targets.Max_Volume:=Targets.\n\teLT_Status.Transfer_Sparge_Active:\n\t\tConfig.LTun.Task_Eta:=TO_UDINT(Sparge.ETA);\n\t\tIF Discharge_Flow.PDO.VolumeFlow.EU>Targets.Max_Flow  \t\t\t\t\t\tTHEN\tGrant_out_ADJ:= LIMIT(-5,Grant_out_ADJ-0.02,5);\tEND_IF;\n\t\tIF Discharge_Flow.PDO.VolumeFlow.EU<Targets.Max_Flow AND Grant_out_ADJ<5\tTHEN\tGrant_out_ADJ:= LIMIT(-5,Grant_out_ADJ+0.02,5);\tEND_IF;\n\t\tLow_Flow_Ton.IN:=Discharge_Flow.PDO.VolumeFlow.EU<Targets.Min_Flow;\n\t\tSparge.Target_Rate:=MIN(Targets.Sparge.Rate,Discharge_Flow.PDO.VolumeFlow.EU);\n\t\tIF (Sparge.Status=eQuick_Mix_Status.Batch_Complete)  THEN\n\t\t\tNext_Status:=eLT_Status.Transfer_Sparge_Complete;\n\t\tEND_IF;\n\t\tIF Low_Flow_Ton.Q  THEN\tNext_Status:=eLT_Status.Low_Flow;\tEND_IF;\n\t\t\n\t\t\nEND_CASE;\n\n\n\n\nConfig.LTun.Status.eStatus:=Test_Sparge();","LineInfoPersistence":"(string)1235aa89-c698-4dae-838a-9d9d6387e61f_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)METHOD Task_Sparge\nVAR_INPUT\n\tTargets: Reference to cLT_Transfer_Sparge;\nEND_VAR\n","LineInfoPersistence":"(string)1235aa89-c698-4dae-838a-9d9d6387e61f_Decl_LineIds"}}}}}},"TypeInfos":{"3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","f8a58466":"{f8a58466-d7f6-439f-bbb8